<div id= 'tracks-container'>
  <% for(var i=1; i<9; i++) {%>
    <div class='track'>
       <div id="waveform<%=i %>" class='wavecontainer waveform'></div>
       <div class='track-controls'>

         <a  class='track-button muteButton' id='muteButton-<%=i %>'>
           <span class='glyphicon glyphicon-volume-up'></span>
         </a>


         <a class='track-button soloButton' id='soloButton-<%=i %>'>solo</a>


         <a class='track-button deleteButton' id='deleteButton-<%=i %>'>
           <span class='glyphicon glyphicon-trash'></span>
         </a>

      </div>
    </div>
  <% } %>
</div>
